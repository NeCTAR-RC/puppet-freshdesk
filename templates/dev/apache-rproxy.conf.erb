# /etc/apache2/conf-available/freshdesk.pip

<VirtualHost *:443>
  DocumentRoot  /var/www/html
  SSLEngine on
  SSLCertificateFile "/opt/nectar/wildcard.crt"
  SSLCertificateKeyFile "/opt/nectar/wildcard.key"

  # Possible values include: debug, info, notice, warn, error, crit,
  # alert, emerg.
  LogLevel warn

  ProxyPreserveHost On

  ProxyPass               /pip http://localhost:<%= @pip_port %>/pip
  ProxyPassReverse        /pip http://localhost:<%= @pip_port %>/pip

  ProxyPass               /osid http://localhost:1338/osid
  ProxyPassReverse        /osid http://localhost:1338/osid
</VirtualHost>

